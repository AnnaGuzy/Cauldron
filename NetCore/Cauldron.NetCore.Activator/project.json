{
  "version": "1.0.0.12",

  "dependencies": {
    "Cauldron.NetCore.Core": "1.0.0.12",
    "Cauldron.NetCore.Interception": "1.0.0.12",
    "NETStandard.Library": "1.6.1",
    "System.Threading.Tasks": "4.3.0",
    "System.Threading.Thread": "4.3.0"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  },

  "buildOptions": {
    "outputName": "Cauldron.Activator",
    "optimize": true,
    "allowUnsafe": false,
    "xmlDoc": true,
    "compile": {
      "include": [
        "../../Shared/Cauldron.Activator/**/*.cs",
        "../../Shared/IEnumerableExtensions/**/*.cs",
        "../../Shared/Cauldron.Diagnostics/**/*.cs"
      ]
    },
    "define": [ "NETCORE" ]
  }
}
<ResourceDictionary x:Class="ViewModelCommunication.Views.ViewsDictionary"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:ViewModelCommunication.Views"
      xmlns:behaviour="clr-namespace:Couldron.Behaviours;assembly=Couldron">

    <DataTemplate x:Key="View_MainViewModel">
        <TabControl Margin="5" ItemsSource="{Binding Animals}" DisplayMemberPath="Name">
            <behaviour:Interaction.Behaviours>
                <behaviour:WindowConfigurationBehaviour Title="Test Title" />
            </behaviour:Interaction.Behaviours>
        </TabControl>
    </DataTemplate>
</ResourceDictionary>
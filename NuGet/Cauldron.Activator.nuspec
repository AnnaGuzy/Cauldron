<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Capgemini.Cauldron.Activator</id>
    <version>1.1.5</version>
    <title>Capgemini Cauldron Activator</title>
    <authors>Capgemini Deutschland GmbH</authors>
    <owners>Capgemini Deutschland GmbH</owners>
    <licenseUrl>https://raw.githubusercontent.com/Capgemini/Cauldron/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/Capgemini/Cauldron</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/Capgemini/Cauldron/master/cauldron.png</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>The activator is a simple and very fast dependency injection framework. It is based on attributes and does not require any configuration files for configuration. It also supports using static methods as component constructor.</description>
    <summary>Simple Dependency Injection Framework</summary>
    <copyright>Copyright (c) 2016 Capgemini Deutschland GmbH</copyright>
    <tags>Toolkit Capgemini Activator Dependency Injection Factory Cauldron</tags>
    <dependencies>
      <group targetFramework="uap10.0">
        <dependency id="Capgemini.Cauldron.Core" version="1.2.16-beta" />
        <dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="5.3.3" />
        <dependency id="Cauldron.Interception.Fody" version="1.2.16-beta" />
        <dependency id="Fody" version="2.1.2" />
      </group>
      <group targetFramework="net461">
        <dependency id="Capgemini.Cauldron.Core" version="1.2.16-beta" />
        <dependency id="Cauldron.Interception.Fody" version="1.2.16-beta" />
        <dependency id="Fody" version="2.1.2" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Capgemini.Cauldron.Core" version="1.2.16-beta" />
        <dependency id="Cauldron.Interception.Fody" version="1.2.16-beta" />
        <dependency id="Fody" version="2.1.2" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- Desktop -->
    <file src="..\Desktop\Cauldron.Desktop.Activator\bin\Release\Cauldron.Activator**" target="lib\net461" />

    <!-- UWP -->
    <file src="..\UWP\Cauldron.UWP.Activator\bin\Release\Cauldron.Activator**" target="lib\uap10.0" />

    <!-- Net Standard 2.0 -->
    <file src="..\NetCore\Cauldron.NetCore.Activator\bin\Release\netstandard2.0\Cauldron.Activator**" target="lib\netstandard2.0" />
  </files>
</package>